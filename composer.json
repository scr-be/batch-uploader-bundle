        "doctrine/doctrine-bundle"          : "~1.4",
        "doctrine/doctrine-fixtures-bundle" : "~2.0",
        "doctrine/data-fixtures"            : "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Scribe\\FileUploaderBundle\\": "src/"

{
	"name"        : "scr-be/batch-uploader-bundle",
	"license"     : "MIT",
	"type"        : "project",
	"homepage"    : "https://scr.be/batch-uploader-bundle",
	"description" : "Bundle to help implement Blueimp's multi-file upload functionality in Symfony.",
	"support" : {
		"email"  : "rmf@scr.be",
		"issues" : "https://scr.be/batch-uploader-bundle/git",
		"source" : "https://scr.be/batch-uploader-bundle/git",
		"irc"    : "irc://irc.efnet.org/scribe"
	},
	"keywords"    : [
		"batch-uploader-bundle", "scribe-batch-uploader-bundle", "blueimp", "bundle", "scr.be", "scribe", "scribenet", "scribe-software", "scribe-systems"
	],
	"authors": [
		{
			"name"     : "Rob Frawley 2nd",
			"email"    : "rmf@scr.be",
			"homepage" : "https://scr.be/rmf",
			"role"     : "Project Lead (of Fork)"
		},
		{
			"name"  : "Thomas Boutell",
			"email" : "tom@punkave.com"
		},
		{
			"name"  : "Wes John-Alder",
			"email" : "wes@punkave.com"
		}
	],
	"require" : {
		"php"                                    : ">=5.6.4,~5.6|~7.0",
		"scr-be/wonka-bundle"                    : "~0.3",
		"scr-be/mantle-bundle"                   : "~2.4",
		"scr-be/teavee-object-cache-bundle"      : "~2.4",
		"scr-be/arthur-doctrine-library"         : "~0.3",
		"scr-be/arthur-doctrine-fixtures-bundle" : "~0.4",
		"scr-be/arthur-doctrine-uuid-library"    : "~0.2"
	},
	"require-dev" : {
        "symfony/symfony"                   : "~2.8|~3.0",
        "symfony/framework-bundle"          : "~2.8|~3.0",
		"symfony/var-dumper"                : "~2.8|~3.0",
		"symfony/monolog-bundle"            : "~2.8|~3.0",
		"symfony/debug-bundle"              : "~2.8|~3.0",
		"symfony/phpunit-bridge"            : "~2.8|~3.0",
		"phpunit/phpunit"                   : "~5.0",
		"codacy/coverage"                   : "~1.0",
		"satooshi/php-coveralls"            : "~0.7",
        "doctrine/orm"                      : "~2.5",
        "doctrine/common"                   : "~2.5",
        "doctrine/dbal"                     : "~2.5",
        "doctrine/doctrine-bundle"          : "~1.4",
        "doctrine/doctrine-fixtures-bundle" : "~2.0",
        "doctrine/data-fixtures"            : "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Scribe\\FileUploaderBundle\\": "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : { "Scribe\\FileUploaderBundle\\Tests\\" : "tests/" }
	},
	"minimum-stability" : "dev",
	"prefer-stable"     : true,
	"config": {
		"bin-dir" : "bin"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.4-dev"
		}
	}
}
